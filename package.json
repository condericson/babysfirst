  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "git add"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/server/"
    ]
  },
  "scripts": {
    "dev:fe": "react-scripts start",
    "deploy:fe": "nps deployFE",
    "dev": "nps dev",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "eject": "react-scripts eject",
    "test": "npm run fe && npm run be",
    "fe": "cross-env CI=true react-scripts test --env=jsdom",
    "be": "cross-env NODE_ENV=test mocha --colors --compilers js:babel-register server/tests -name '*.test.js')",
    "test:server": "cross-env NODE_ENV=test mocha -w --colors --compilers js:babel-register $(find server/specs -name '*.specs.js')",
    "mochatest": "mocha",
    "build:server": "nps buildServer",
    "prettier": "node ./scripts/prettier.js write",
    "hello": "rimraf node_modules && rimraf yarn.lock",
    "lint-staged": "lint-staged",
    "lint": "eslint src server"
  }
}
